{
  "name": "service-poster",
  "version": "1.0.0",
  "description": "Servicio posteador",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch build ./build/index.js --exec babel-node",
    "build": "nodemon --ext ts --declaration --watch ./src/ --ignore node_modules/ --ignore .git --ignore build/ --exec tsc"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gastonrd7/service-poster.git"
  },
  "author": "RuizDiaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gastonrd7/service-poster/issues"
  },
  "homepage": "https://github.com/gastonrd7/service-poster#readme",
  "devDependencies": {
    "@types/node": "^10.12.12",
    "@types/request-promise": "^4.1.42",
    "@types/uuid": "^3.4.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "dotenv": "^6.2.0",
    "influencers-models": "^1.1.10",
    "influencers-service-bus": "^1.0.13",
    "inversify": "^4.14.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.3.0",
    "reflect-metadata": "^0.1.12",
    "ts-nats": "^1.1.2",
    "typescript": "^3.1.3",
    "winston": "^3.2.1",
    "cron": "^1.7.0",
    "selenium-webdriver": "^4.0.0-alpha.1"
  }
}
